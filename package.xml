  <name>sfCkPlugin</name>
  <channel>plugins.symfony-project.org</channel>
  <summary>Unobtrousive integration between symfony and CKEditor</summary>
  <description>This plugins offers an unobtrousive integration between the CK WYSIWYG editor and symfony.</description>
  <lead>
    <name>Alessandro Nadalin</name>
    <user>odino</user>
    <email>alessandro.nadalin@gmail.com</email>
    <active>yes</active>
  </lead>
  <date>2010-09-22</date>
  <version>
    <release>1.0.0</release>
    <api>1.0.0</api>
  </version>
  <stability>
    <release>stable</release>
    <api>stable</api>
  </stability>
  <license uri="http://www.symfony-project.org/license">MIT license</license>
  <notes>
    * see plugin README on github
  </notes>
  <contents>
  <dir name="/">
    <file role="data" name="README" />
    <file role="data" name="README.md" />
    <file role="data" name="LICENSE" />
    <file role="data" name="VERSION" />
    <dir name="web">
      <dir name="js">
        <file role="data" name="sfCkPlugin.js" />
      </dir>
    </dir>
  </dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.2.4</min>
      </php>
      <pearinstaller>
        <min>1.4.1</min>
      </pearinstaller>
      <package>
        <name>symfony</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.4.0</min>
        <max>1.5.0</max>
        <exclude>1.5.0</exclude>
      </package>
    </required>
  </dependencies>
